// Both appsettings.json and appsettings.development.json should have this structure
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "<CONNECTION_STRING_NAME>": "<CONNECTION_STRING>"
  }
}
